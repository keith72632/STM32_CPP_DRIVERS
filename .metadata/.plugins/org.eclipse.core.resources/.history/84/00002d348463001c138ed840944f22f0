#ifndef SYSTICK_H_
#define SYSTICK_H_

#include <stdint.h>

namespace Systick {

	typedef struct SYST_CSR {
		uint32_t enable        :1;
		uint32_t tick_int      :1;
		uint32_t clk_source    :1;
	}SYST_CSR_t;

	typedef struct systick {

	}__attribute__((packed))systick_t;
}


#endif /* SYSTICK_H_ */
