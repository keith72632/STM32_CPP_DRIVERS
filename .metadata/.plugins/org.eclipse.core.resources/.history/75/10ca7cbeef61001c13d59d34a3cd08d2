#include <stdint.h>
#include <stdio.h>
#include "scb.h"

int main(void)
{
    /* Loop forever */
	SCB::SHCRS_t *shcrs = (SCB::SHCRS_t*)0xE000ED24;
	shcrs->mem_fault_enable();
	shcrs->bus_fault_enable();
	shcrs->usg_fault_enable();

	void (*addr)(void);
	uint32_t *SRAM = (uint32_t*)0x20010000;
	addr = (void(*))SRAM;
	addr();
	for(;;);
}
