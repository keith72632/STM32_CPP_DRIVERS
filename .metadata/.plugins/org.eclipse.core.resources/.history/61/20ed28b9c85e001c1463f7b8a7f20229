#ifndef RCC_H_
#define RCC_H_

#include <stdint.h>

namespace Rcc {
	struct AHB1_ENR{
		AHB1_ENR();
		uint32_t gpioa :1;
		uint32_t gpiob :1;
		uint32_t gpioc :1;
		uint32_t gpiod :1;
		uint32_t gpioe :1;
		uint32_t gpiof :1;
		uint32_t space :26;
	};
} /* namespace Rcc */

#endif /* RCC_H_ */
